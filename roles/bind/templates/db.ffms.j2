; zonefile for ffms. for {{item}}
$TTL    86400
@       IN      SOA     ffms. root.ffms. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                          86400 )       ; Negative Cache TTL
;
@			IN      NS      {{bind.ns_name}}.ffms.
{{bind.ns_name}}			IN      A       {{hostvars[groups[item][0]].ff_network.v4_network | ipaddr(server_id) | ipaddr('address') }}
node			IN      A       {{hostvars[groups[item][0]].ff_network.v4_network | ipaddr(1) | ipaddr('address')}}
node			IN      AAAA    {{hostvars[groups[item][0]].ff_network.v6_network | ipaddr(1) | ipaddr('address')}}
firmware	IN      A       144.76.30.226
firmware	IN      AAAA    2a01:4f8:191:21e1::23
